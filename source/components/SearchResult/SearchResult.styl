.searchResult {
	min-width 200px
	display inline-block
	margin-bottom 20px
	padding 10px
	border 2px solid #5fb3e4
	box-sizing border-box
	border-radius 5px
	&__temp {
		margin-bottom 10px
	}
	&__name {
		color black
		font-size 20px
		margin-bottom 10px
	}
	&__btn {
		display block
		padding 7px 15px
		letter-spacing 2px	
		cursor pointer
	    background green
	    border none
	    color white
	}
}
